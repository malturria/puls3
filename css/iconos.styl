@font-face {
  font-family: 'fpuls3';
  src:url('../fonts/fpuls3.eot');
  src:url('../fonts/fpuls3.eot?#iefix') format('embedded-opentype'),
    url('../fonts/fpuls3.woff') format('woff'),
    url('../fonts/fpuls3.ttf') format('truetype'),
    url('../fonts/fpuls3.svg#fpuls3') format('svg');
  font-weight: normal;
  font-style: normal;
}

.icon-fpuls3home, .icon-fpuls3github {
  font-family: 'fpuls3';
  speak: none;
  font-size: 1.5em;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
}
.icon-fpuls3home:before {
  content: "\e000";
}
.icon-fpuls3github:before {
  content: "\e001";
}
